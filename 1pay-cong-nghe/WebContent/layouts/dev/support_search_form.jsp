<%@ page language="java" trimDirectiveWhitespaces="true" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>

<div class="row">
<div class="col-md-12">
	<form name="support_search" action="<%=request.getContextPath() %>/dev/support.dev" method="POST">
		<div class="input-group" style="text-align: left">
			<input type="text" class="form-control" name="s" value="${param.s}" id="key_word" placeholder="Tìm kiếm tài liệu, câu hỏi, trả lời..."> 
			<span class="input-group-btn">
				<a href="javascript:$('form[name=support_search]').submit();" class="btn green">
					<i class="fa fa-search"></i> Tìm kiếm
				</a>
			</span>
		</div>
	</form>
	<br />
</div>
</div>